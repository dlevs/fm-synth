{
  "name": "fm-synth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build -- --watch & (sleep 2 && npm run serve:watch)",
    "build": "babel src/ --out-dir dist --extensions .ts,.tsx,.js,.jsx",
    "serve:watch": "live-server --ignore='**/*.tsx'",
    "pack": "snowpack --source pika",
    "storybook": "start-storybook",
    "build-storybook": "npm run lint && build-storybook -s public",
    "lint": "eslint 'src/**/*.+(ts|tsx|js|jsx)'"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@reduxjs/toolkit": "1.3.2",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "lodash-es": "4.17.15",
    "parse-midi": "2.0.2",
    "polished": "3.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@types/jest": "25.2.1",
    "@types/lodash-es": "4.17.3",
    "@types/node": "13.11.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "7.1.7",
    "@types/storybook__addon-actions": "5.2.1",
    "@types/storybook__addon-knobs": "5.2.1",
    "@types/storybook__react": "5.2.1",
    "@types/webmidi": "2.0.4",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-loader": "8.1.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-transform-node-env-inline": "0.4.3",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "eslint-plugin-standard": "4.0.1",
    "hooks.macro": "1.0.1",
    "live-server": "1.2.1",
    "snowpack": "1.6.0",
    "typescript": "3.8.3"
  }
}
